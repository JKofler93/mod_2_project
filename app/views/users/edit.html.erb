<h1>Update User: </h1>

<% if flash[:errors]%>
<% flash[:errors].each do |error|%>
<%= error%>
<%end%>
<%end%>


<%=form_for @user do |f|%>
<%=f.label "Username:"%>
<%=f.text_field :user_name%>

<%=f.label "Age:"%>
<%=f.number_field :age%>

<%=f.label "Password:"%>
<%=f.password_field :password_digest%>

<%=f.submit "Submit"%>

<%end%>